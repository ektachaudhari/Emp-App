<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.auskeny.ems.hibernate.pojo.Employee" table="employee" catalog="ems">
        <id name="empIdPk" type="integer">
            <column name="empId" />
        </id>
        <property name="empName" type="string">
            <column name="empName" length="45" not-null="true"  />
        </property>
        <property name="empEmail" type="string">
            <column name="empEmail" length="45" not-null="true" />
        </property>
               
    </class>
</hibernate-mapping>
